<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT Company (Name,NoOfDep,Department+)>
<!ATTLIST Company cid CDATA #FIXED>
<!ELEMENT Name (#PACDATA)>
<!ELEMENT Department (DName,NoOfEmp,Emp+)>
<!ATTLIST Department did CADATA #FIXED>
<!ELEMENT DName (#PCDATA)>
<!ELEMENT NoOfEmp (#PCDATA)>
<!ELEMENT Emp (Ename,Sal,Address,DOj,Designation)>
<!ATTLIST Emp Eid CDATA #FIXED>
<!ATTLIST Emp did CDATA #FIXED>
<!ELEMENT Ename (#PCDATA)>
<!ELEMENT Sal (#PCDATA)>
<!ELEMENT Address (Street1,Street2?,City,State,Country)>
<!ELEMENT DOJ (Day,Month,Year)>
<!ELEMENT Street1 (#PCDATA)>
<!ELEMENT Street2 (#PCDATA)>
<!ELEMENT City (#PCDATA)>
<!ELEMENT State (#PCDATA)>
<!ELEMENT Country (#PCDATA)>
<!ELEMENT Day (#PCDATA)>
<!ELEMENT Month (#PCDATA)>
<!ELEMENT Year (#PCDATA)>
<!ELEMENT Designation (#PCDATA)>
 
